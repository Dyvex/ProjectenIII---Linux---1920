# MariaDB Custom server configuration
#
# {{ ansible_managed }}
{% for section_name, section_contents in mariadb_custom_cnf.items() %}
[{{ section_name }}]
{% for key, value in section_contents.items() %}
{% if value is defined and value %}
{{ key }}={{ value }}
[client]
user = root
password= "admin"
host = localhost
{% else %}
{{ key }}
{% endif %}
{% endfor %}

{% endfor %}

